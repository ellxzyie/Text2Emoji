local ExecuteList = function()
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextBox = Instance.new("TextBox")
local TextButton = Instance.new("TextButton")
local ScrollingFrame = Instance.new("ScrollingFrame")
local UIListLayout = Instance.new("UIListLayout")

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

Frame.Parent = ScreenGui
Frame.Active = true
Frame.AnchorPoint = Vector2.new(0.5, 0.5)
Frame.BackgroundColor3 = Color3.fromRGB(91, 91, 91)
Frame.Position = UDim2.new(0.499448121, 0, 0.49899599, 0)
Frame.Size = UDim2.new(0, 216, 0, 279)

TextBox.Parent = Frame
TextBox.BackgroundColor3 = Color3.fromRGB(166, 166, 166)
TextBox.BorderSizePixel = 4
TextBox.Position = UDim2.new(0.0370370373, 0, 0.0215053763, 0)
TextBox.Size = UDim2.new(0, 165, 0, 28)
TextBox.Font = Enum.Font.SourceSans
TextBox.Text = ""
TextBox.TextColor3 = Color3.fromRGB(0, 0, 0)
TextBox.TextSize = 14.000

TextButton.Parent = Frame
TextButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
TextButton.Position = UDim2.new(0.833333373, 0, 0.0215053782, 0)
TextButton.Size = UDim2.new(0, 30, 0, 28)
TextButton.Font = Enum.Font.SourceSans
TextButton.Text = "X"
TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
TextButton.TextSize = 18.000
TextButton.TextWrapped = true

ScrollingFrame.Parent = Frame
ScrollingFrame.Active = true
ScrollingFrame.BackgroundColor3 = Color3.fromRGB(115, 115, 115)
ScrollingFrame.Position = UDim2.new(0, 0, 0.157706097, 0)
ScrollingFrame.Size = UDim2.new(0, 216, 0, 233)
ScrollingFrame.CanvasPosition = Vector2.new(0, 900)
ScrollingFrame.AutomaticCanvasSize = Enum.AutomaticSize.Y
ScrollingFrame.ScrollBarThickness = 7

UIListLayout.Parent = ScrollingFrame
UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder

-- TextBox.LocalScript 
local script = Instance.new('LocalScript', TextBox)

local TextBox = script.Parent
local ScrollingFrame = script.Parent.Parent.ScrollingFrame

local function filterItems(searchTerm)
	searchTerm = string.lower(searchTerm) 
	local matches = {}

	for _, item in ipairs(ScrollingFrame:GetChildren()) do
		if item:IsA("TextLabel") then 
			local itemName = string.lower(item.Text)

			if string.find(itemName, searchTerm) then
				item.Visible = true
				table.insert(matches, item)
			else
				item.Visible = false
			end
		end
	end

	for i, item in ipairs(matches) do
		item.LayoutOrder = i 
	end
end

TextBox:GetPropertyChangedSignal("Text"):Connect(function()
	local searchTerm = TextBox.Text
	filterItems(searchTerm)
end)

-- TextButton.LocalScript 
local script = Instance.new('LocalScript', TextButton)

local frame = script.Parent.Parent 

local function deleteGUI()
	frame.Parent:Destroy() 
end

script.Parent.MouseButton1Click:Connect(deleteGUI)
-- ScrollingFrame.LocalScript 
local script = Instance.new('LocalScript', ScrollingFrame)

local function createTextLabels(scrollingFrame, texts)
	for index, text in ipairs(texts) do
		local textLabel = Instance.new("TextLabel")
		textLabel.Name = "Label" .. index
		textLabel.Parent = scrollingFrame
		textLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		textLabel.Position = UDim2.new(0, 0, 0)
		textLabel.Size = UDim2.new(1, 0, 0, 20)
		textLabel.Font = Enum.Font.SourceSans
		textLabel.Text = text
		textLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
		textLabel.TextSize = 14.0
		textLabel.TextScaled = true
	end
end


